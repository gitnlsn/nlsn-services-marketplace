{
	"name": "savoir-link",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"db:drop": "prisma migrate reset",
		"db:push": "prisma db push",
		"docker": "sudo docker compose up && sudo docker compose down -v",
		"dev": "next dev --turbo",
		"mermaid": "node scripts/validate-mermaid.js",
		"postinstall": "prisma generate",
		"preview": "next build && next start",
		"start": "next start",
		"test:frontend": "dotenv -e .env.test -- vitest run --config vitest.config.frontend.ts",
		"test:backend": "dotenv -e .env.test -- vitest run --config vitest.config.backend.ts",
		"test:all": "dotenv -e .env.test -- npm run test:backend && npm run test:frontend",
		"test:e2e": "npx tsx e2e/tests/landing-page.test.ts",
		"test:e2e:auth": "npx tsx e2e/tests/auth-flow.test.ts",
		"test:e2e:navigation": "npx tsx e2e/tests/navigation.test.ts",
		"test:e2e:performance": "npx tsx e2e/tests/performance.test.ts",
		"test:full": "npx tsx scripts/run-full-test-suite.ts",
		"typecheck": "biome check --write --unsafe . && tsc --noEmit",
		"check": "npm run mermaid && npm run typecheck && npm run build && npm run test:all"
	},
